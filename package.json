    "author": "YuZesky <yuri.delzesky@gmail.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/my-avatar/agency-plus.git"
    },
    "dependencies": {
        "bootstrap": "^5.2.0",
        "is-undefined": "^1.0.11",
        "merge-objects": "^1.0.5",
        "react": "^18.2.0",
        "react-bootstrap": "^2.5.0"
    },
    "devDependencies": {
        "@sapphire/prettier-config": "^1.4.3",
        "@types/merge-objects": "^1.0.0",
        "@types/node": "^18.7.6",
        "@types/react": "^18.0.17",
