{
    "title": "Agency Plus",
    "name": "@my-avatar/agency-plus",
    "version": "1.0.0",
    "scripts": {
        "build": "npm run clean && npm run build:pug && npm run build:scss && npm run build:scripts && npm run build:assets",
        "start": "npm run build && node scripts/start.js",
        "format": "prettier --config ./scripts/prettier.config.js --write \"{assets,scripts,src}/**/*.{ts,tsx,scss}\""
    },
    "description": "Agency Plus is a one page HTML theme for Bootstrap.",
